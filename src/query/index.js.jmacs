const stream = require('stream');

const pattern = require('./pattern.js');

class store_sync {
	constructor(k_store) {
		Object.assign(this, {
			store: k_store,
		});
	}

	pattern() {
		return new pattern(this.store);
	}
}

module.exports = {
	sync(k_store) {
		return new store_sync(k_store);
	},
};
