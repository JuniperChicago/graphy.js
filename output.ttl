@prefix dt: <http://dbpedia.org/datatype/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbp: <http://dbpedia.org/property/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix color: <http://example.org/color#> .
@prefix umbel-rc: <http://umbel.org/umbel/rc/> .

dbr:Banana owl:sameAs <http://wikidata.dbpedia.org/resource/> ;
	rdf:type owl:Thing, umbel-rc:Plant, umbel-rc:EukaryoticCell, umbel-rc:BiologicalLivingObject ;
	dbo:category <http://dbpedia.org/resource/Category/Edible_fruits> ;
	dbo:ingredientOf <http://dbpedia.org/resource/Banana_boat_(food)> ;
	rdfs:comment "Bananas are amazing." ;
	dbo:abstract "Bananas, pronounced \"bəˈnanəz\", are yellow!" ;
	rdfs:label "Banana"@en, "Banane"@fr, "Banaan"@nl ;
	dbp:fiber "2.6"^^dt:gram ;
	dbp:protein "1.09"^^dt:gram ;
	dbp:sugars "12.23"^^dt:gram ;
	dbp:taste _:good_taste ;
	dbp:color [
		color:asHexadecimal "ffd700" ;
		color:asRGB [
			color:red "255"^^xsd:integer ;
			color:green "215"^^xsd:integer ;
			color:blue "0"^^xsd:integer
		]
	], [
		$ "peel", "slice", "distribute"
	] .

dbr:Blake dbp:test "Blake" ;
	dbp:bn [
		dbp:deep "init" ;
		dbp:test "true" ;
		dbp:init [
			dbp:to "winnit"
		]
	] .
